<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
        </goals>
        <properties>
            <!--spring.profiles.active=js-->
            <exec.args>-classpath %classpath com.sjr.kimosyst.Application --spring.profiles.active=007</exec.args>
            <exec.executable>java</exec.executable>
        </properties>
    </action>  
</actions>
